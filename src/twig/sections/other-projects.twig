<section class="other-projects  list-section  js-section" data-speed="0">
    <div class="container-fluid">
        <div class="row">
            <div class="col-xs-12  col-sm-offset-1  col-sm-10  col-md-10  col-lg-offset-2  col-lg-8">
                <h2 class="title  js-item">
                    {{- 'Other projects' -}}
                </h2>
                <ul class="list  row">
                    {% for project in data.projects if project.description is empty %}
                        <li class="list__item  col-xs-12">
                            <h3 class="js-item">
                                <a class="list__link" href="{{ project.url }}" target="_blank" rel="noopener">
                                    {{- project.title -}}<sup>{{- project.year -}}</sup>
                                </a>
                            </h3>
                        </li>
                    {% endfor %}
                    <li class="list__item  col-xs">
                        <span class="js-item">
                            {{- 'â€¦' -}}
                        </span>
                    </li>
                </ul>
            </div>
        </div>
    </div>
</section>
